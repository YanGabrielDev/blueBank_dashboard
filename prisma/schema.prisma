datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" 
}

generator client {
  provider = "prisma-client-js"
}

model CustomerGrowth {
  id      Int    @id @default(autoincrement())
  month   String
  clients Int
  year    Int
}

model CustomerGrowthInsert {
  id        Int      @id @default(autoincrement()) // Id auto-increment√°vel
  month     String
  clients Int
}